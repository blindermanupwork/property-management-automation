# Development Environment Configuration
# Copy this file to .env and fill in your actual development values

ENVIRONMENT=development

# Development Airtable (separate from production)
AIRTABLE_API_KEY=your_dev_airtable_api_key_here
AIRTABLE_BASE_ID=your_dev_airtable_base_id_here
AIRTABLE_TABLE_NAME=Reservations_Dev

# Use test data and lower frequency for development
FETCH_RESERVATIONS_MONTHS_BEFORE=1
IGNORE_BLOCKS_MONTHS_AWAY=3