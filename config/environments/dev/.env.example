# HousecallPro MCP Server - Development Environment Configuration
# Copy this file to .env and fill in your actual values

# HousecallPro API Configuration
DEV_HCP_TOKEN=tok_your_dev_token_here
DEV_HCP_BASE_URL=https://api.housecallpro.com
DEV_HCP_EMPLOYEE_ID=pro_your_employee_id_here
DEV_HCP_DEFAULT_EMPLOYEE_ID=pro_your_default_employee_id_here
DEV_HCP_WEBHOOK_SECRET=your_dev_hcp_webhook_secret_here

# Webhook Configuration
DEV_SERVATIV_WEBHOOK_SECRET=your_dev_servativ_webhook_secret_here

# Rate Limiting Configuration
DEV_HCP_RATE_LIMIT=60
DEV_HCP_RETRY_AFTER_MS=1000

# Optional: Logging Level
LOG_LEVEL=info